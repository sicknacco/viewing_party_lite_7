<% if current_user %>
  <%= link_to "Log Out" %>
<% else %>
  <div id="new_user">
    <%= button_to "Create New User", "/register", method: :get %>
    <%= button_to "Log In", login_path, method: :get %>
  </div>
  <% end %>

<ul id="existing_users">
  <h3>Existing Users</h3>
  <% @users.each do |user| %>
    <li>
      <% if current_user %>
        <%= link_to user.email, dashboard_path(user.id) %>
      <% else %>
        <%= link_to user.email, login_path %>
      <% end %>
    </li>
  <% end %>
</ul>