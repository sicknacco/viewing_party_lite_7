<% if current_user %>
  <%= button_to "Log Out", logout_path, method: :delete %>
<% else %>
  <div id="new_user">
    <%= button_to "Create New User", "/register", method: :get %>
    <%= button_to "Log In", login_path, method: :get %>
  </div>
<% end %>

<% if current_user %>
  <ul id="existing_users">
    <h3>Existing Users</h3>
      <% @users.each do |user| %>
        <li><%= user.email %></li>
      <% end %>
  </ul>
<% end %>